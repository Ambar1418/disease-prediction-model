{
  "version": 2,
  "builds": [
    {
      "src": "core/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "python": {
          "runtime": "python3.11"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/core/wsgi.py"
    }
  ]
}
